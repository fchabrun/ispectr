{
	"architecture": "swinunet",
	"architecture_config": {
		"spe_size": 304,
		"patch_size": 4,
		"in_chans": 6,
		"num_classes": 5,
		"embed_dim": 64,
		"depths": [4, 4, 4],
		"depths_decoder": [1, 2, 2],
		"num_heads": [8, 8, 8],
		"window_size": 19,
		"mlp_ratio": 4.0,
		"drop_rate": 0.0,
		"attn_drop_rate": 0.0,
		"drop_path_rate": 0.1,
		"ape": false,
		"patch_norm": true
	},
	"model_config": {
		"optimizer": "AdamW",
		"lr_scheduler": "cosine_with_restarts",
		"lr": 5e-4,
		"lr_reduceonplateau_factor": 0.5,
		"lr_reduceonplateau_patience": 5,
		"lr_reduceonplateau_threshold": 1e-2,
		"lr_reduceonplateau_minlr": 1e-6,
		"num_warmup_steps": 2700,
		"num_training_steps": 43200,
		"adam_eps": 1e-08,
		"weight_decay": 0.0
	},
	"callbacks_config": {
		"monitor": "val_loss",
		"mode": "min",
		"early_stopping_min_delta": 1e-4,
		"early_stopping_patience": 50
	},
	"max_epochs": 80,
	"batch_size": 32,
	"coarse_dropout": false,
	"smoothing": false,
	"online_normalize": false,
	"permutation": "gam_kl"
}